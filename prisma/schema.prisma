generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Transaction {
  id         Int      @id @default(autoincrement())
  userKey    String?   @db.VarChar(64)
  amount     Int
  category   String
  type       String
  createdAt  DateTime @default(now())
  occurredAt DateTime @default(now())

  @@index([userKey, occurredAt])
}